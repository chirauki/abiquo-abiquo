{
  "name": "abiquo-abiquo",
  "version": "0.1.6",
  "author": "Marc Ciraququi <marc.cirauqui@abiquo.com>",
  "license": "Apache-2.0",
  "summary": "Allows to setup basic Abiquo components",
  "source": "git@github.com:chirauki/abiquo-abiquo.git",
  "project_page": "https://github.com/chirauki/abiquo-abiquo",
  "issues_url": "https://github.com/chirauki/abiquo-abiquo/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS", "RedHat"
      "operatingsystemrelease": [
        "6"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">= 1.5.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.1.0"
    },
    {
      "name": "puppetlabs/firewall",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "jfryman/selinux",
      "version_requirement": ">= 0.2.2"
    },
    {
      "name": "thias/sysctl",
      "version_requirement": ">= 1.0.2"
    },
    {
      "name": "camptocamp/openssl",
      "version_requirement": ">= 0.3.1"
    },
    {
      "name": "locp/cassandra",
      "version_requirement": ">= 1.25.1"
    },
    {
      "name": "jmkeyes/kairosdb",
      "version_requirement": ">= 0.0.6"
    },
    {
      "name": "puppetlabs/hocon",
      "version_requirement": ">= 0.9.4"
    }
  ]
}
